{
  "version": 1,
  "paths": {
    "/menu": {
      "name": "/menu",
      "lambdaFunction": "menuFunction",
      "permissions": {
        "setting": "open"
      }
    },
    "/reviews": {
      "name": "/reviews",
      "lambdaFunction": "reviewFunction",
      "permissions": {
        "setting": "protected",
        "auth": [
          "create",
          "read",
          "update",
          "delete"
        ],
        "guest": [
          "read"
        ]
      }
    }
  }
}